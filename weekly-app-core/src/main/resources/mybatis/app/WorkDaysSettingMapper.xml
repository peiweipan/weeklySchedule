<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.weekly.mapper.WorkDaysSettingMapper">

    <select id="getWorkDaysBySetting" resultType="INTEGER">
        select work_days from workDays_setting
        where schoolyear_semester = #{schoolYear} and week = #{week}
    </select>

</mapper>

